<?xml version="1.0" encoding="UTF-8"?>

<configuration>

    <appender name="Console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%level %date{dd:MM:yyyy HH:mm:ss} [%thread] %logger{36} - %message%n</pattern>
        </encoder>
    </appender>

    <root level="off">
        <appender-ref ref="Console" />
    </root>

</configuration>